#include <iostream>
#include <cstring>

using namespace std;

char dst[100];
char str2[30];
int dlina;

char *rev(char *src){
  memset(dst,0,100);
  for(int i=strlen(src)-1, a=0; i>=0; i--, a++) 
    dst[a] = src[i];

	return dst;
}

void funkciya(char *stroka){

    char str1[30];
    memset(str1,0,30);
    
    strncpy(str1,stroka,5);
    strcat(str2,rev(str1));

    for(int i=0; i<=dlina; i++)
    {
        if(i<=(dlina-5)){
                stroka[i]=stroka[i+5];
        }else{stroka[i]='\0';}
    }
    
    if(strlen(stroka)>=5) funkciya(stroka);
    else strcat(str2,rev(stroka));


}

int main(){
    char stroka[30]="1234512345";
    memset(str2,0,30);
    dlina=strlen(stroka);
    
    funkciya(stroka);
    
    cout << str2;
    
    return 0;
}
