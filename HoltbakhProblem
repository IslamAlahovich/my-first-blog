#include <stdlib.h>
#include <stdio.h>
#include <iostream>
#include <math.h>

using namespace std;

int n1,n2;

int simple(int n) {
  int i,l=floor(sqrt(n));
  if (n<2) return 0;
  else if (n<4) return 1;
  else if (n%2==0) return 0;
  for (i=3; i<=l; i+=2) if (n%i==0) return 0;
  return 1;
}

int goldbah(int n) {
  int pol=n/2, r=0; n1=2;
  for (n1; n1<=pol; n1++){
    n2=n-n1;
    if (simple(n1) and simple(n2)) {
      cout << n << " = " << n1 << " + " << n2 << endl;
      r++;
    }
  }
  return r;
}

int main () {
  int n=25;
  if (n<4 or n%2!=0) cout << "error" << endl;
  else{
    int found=goldbah(n); 
    if (!found) cout << "not found" << endl;
  }

  return 0;
}
