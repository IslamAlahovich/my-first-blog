#include <iostream>
#include <string>
#include <vector>
#include <set>

using namespace std;
 
void Generate(const string &s, const size_t N, const size_t k = 0)
{
    static vector<int> X(N);
    if (k == N)
    {
        set<int> ss(X.begin(), X.end());
        if (ss.size() == N)
        {
            for (size_t i = 0; i < N; i++)
                cout << s[X[i]];
            cout << endl;
        }
    }
    else
    {
        for (size_t j = 0; j < s.length(); j++)
        {
            X[k] = j;
            Generate(s, N, k + 1);
        }
    }
}
 
int main()
{
    Generate("abc", 3);
}
