#include <iostream>
#include <string>
#include <vector>
 
void Generate(const std::string &s, const size_t N, const size_t k = 0)
{
    static std::vector<int> X(N);
    if (k == N)
    {
        for (const auto x : X)
            std::cout << s[x];
        std::cout << std::endl;
    }
    else
    {
        for (size_t j = 0; j < s.length(); j++)
        {
            X[k] = j;
            Generate(s, N, k + 1);
        }
    }
}
 
int main()
{
    Generate("abcd", 3);
}
