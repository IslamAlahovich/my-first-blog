#include <iostream>
#include <cstring>
#include <set>
using namespace std;

set<char> mySet;

void combo(char str1[],char str2[],int k){
    if(k){
        for(int i=0; i<strlen(str2); i++){
            if(strchr(str1,str2[i])) continue;
            else combo(strcat(str1,str2[i]),str2,k-1);
          }
      }
    else mySet.insert(str1);
  }
int main(){
  char a[]="abc";
  char buff[10];
  memset(buff,0);
  int s=3;
  for(int u=0; u<strlen(a); u++){
      buff[0]=a[u];
      combo(buff,a,s);
    }
  
  copy( mySet.begin(), mySet.end(), ostream_iterator<char>(cout, " "));
  return 0;
}
