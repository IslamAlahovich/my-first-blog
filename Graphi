#include <math.h>
#include <iostream>

using namespace std;

int Pytin(int n, int i, int j,int x[][100] ) {
  int s;
  s=x[i][j];

  for (int u=0;u<n;u++){
    if (x[i][u]+ x[u][j]<s)
      s= x[i][u]+ x[u][j];
  }
  return s;
}

int main() {
  int f,n,i,j,x,y,z;
  cin >> n;
  int m [100][100];

  for (i=0;i<n;i++){
    for (j=0;j<n;j++){
      if (i==j){
        m[i][j]=0;
      } else m[i][j]=0;
    }
  }

  do {
    cin >> x >> y >> z;
    m[x][y]=z;
  } while ((x!=0)||(y!=0)||(z!=0));


  int h[100][100];

  for (f=1;f<n-1;f++){
    for (i=0;i<n;i++){
      for (j=0;j<n;j++){
        h[i][j]=Pytin(n,i,j,m);

      }
    }
    for (i=0;i<n;i++){
      for (j=0;j<n;j++){
        m[i][j]=h[i][j];

      }
    }
  }
    cout << "   ";
    for (int i=0;i<n;i++){cout << i << "  ";}
  for (i=0;i<n;i++){
    cout << "\n";
    cout << i << ": ";
    for (j=0;j<n;j++){
        h[j][i]=h[i][j];
        cout << h[i][j] << " ";
    }
  }

  return 0;
}
