#include <iostream>
#include <string>
using namespace std;
int main() {
  string buff="123/*12345*/abc/*asdfaksjdf23*/asfg";
    for(int i=0;i<buff.size();i++){
        
        if(buff[i]=='/' and buff[i+1]=='*'){
            int k=i+1,m=0,n=1;
            char c[]="01";
            
            for(k;k<buff.size();k++){
                c[0]=buff[k];
                c[1]=buff[k+1];
                if(c[0]=='*' and c[1]=='/'){
                    k=buff.size();
                  }
                m++;
                
            }
            buff.erase(i,m+2);
        }
    }
cout << buff;
return 0;
}
