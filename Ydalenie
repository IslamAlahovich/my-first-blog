#include <iostream>
#include <string.h>
using namespace std;
int main() {
  char buff[50]="1232435sdfg/*asdf23424f*/as234234sdfg5";
  for(int i=0;i<strlen(buff);i++){
      if(buff[i]=="/" and buff[i+1]=="*"){
          int k=i,m=0;
          char c[2];
          while(c[0]!="*" and c[1]!="/"){
              c[0]=buff[k];
              c[1]=buff[k+1];
              m++;
              k++;
            }
          for(i; i<strlen(buff);i++){
              buff[i]=buff[i+m];
            }
          while(m!=0){
              buff[strlen(buff)-1-m]="\0";
              m-=1;
            }
        }
    }
    cout << buff << endl;
  return 0;
}
